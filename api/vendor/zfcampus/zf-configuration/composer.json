{
    "name": "zfcampus/zf-configuration",
    "description": "ZF2 Module providing REST resource for manipulating configuration",
    "type": "library",
    "license": "BSD-3-Clause",
    "keywords": [
        "zf2",
        "zend",
        "module",
        "rest",
        "config"
    ],
    "homepage": "http://apigility.org/",
    "support": {
        "email": "apigility-users@zend.com",
        "irc": "irc://irc.freenode.net/apigility",
        "source": "https://github.com/zfcampus/zf-configuration",
        "issues": "https://github.com/zfcampus/zf-configuration/issues"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev",
            "dev-develop": "1.2-dev"
        }
    },
    "require": {
        "php": ">=5.5",
        "zendframework/zend-config": "~2.3",
        "zendframework/zend-modulemanager": "~2.3",
        "zendframework/zend-stdlib": "~2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.7",
        "squizlabs/php_codesniffer": "^2.3.1",
        "zendframework/zend-loader": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "ZF\\Configuration\\": "src/"
        },
        "classmap": [
            "Module.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "ZFTest\\Configuration\\": "test/"
        }
    }
}
