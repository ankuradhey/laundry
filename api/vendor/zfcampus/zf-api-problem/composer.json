{
    "name": "zfcampus/zf-api-problem",
    "description": "ZF2 Module providing API-Problem assets and rendering",
    "type": "library",
    "license": "BSD-3-Clause",
    "keywords": [
        "zf2",
        "zend",
        "module",
        "rest",
        "api-problem"
    ],
    "homepage": "http://apigility.org/",
    "support": {
        "email": "apigility-users@zend.com",
        "irc": "irc://irc.freenode.net/apigility",
        "source": "https://github.com/zfcampus/zf-api-problem",
        "issues": "https://github.com/zfcampus/zf-api-problem/issues"
    },
    "repositories": [
        { "type": "composer", "url": "https://packages.zendframework.com" }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev",
            "dev-develop": "1.2-dev"
        }
    },
    "require": {
        "php": ">=5.5",
        "zendframework/zend-eventmanager": "~2.3",
        "zendframework/zend-http": "~2.3",
        "zendframework/zend-json": "~2.3",
        "zendframework/zend-mvc": "~2.3",
        "zendframework/zend-view": "~2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.7",
        "zendframework/zend-console": "~2.3",
        "zendframework/zend-loader": "~2.3",
        "squizlabs/php_codesniffer": "^2.3.1"
    },
    "autoload": {
        "psr-4": {
            "ZF\\ApiProblem\\": "src/"
        },
        "classmap": [
            "Module.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "ZFTest\\ApiProblem\\": "test/"
        }
    }
}
